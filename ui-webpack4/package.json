{
  "name": "api-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8081",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^6.7.1",
    "axios": "^0.19.0",
    "consul": "^0.34.1",
    "core-js": "^3.6.4",
    "echarts": "^4.2.1",
    "highlight.js": "^9.15.8",
    "iview": "^3.2.2",
    "json-markup": "^1.1.3",
    "linq": "^3.2.0",
    "oidc-client": "^1.8.2",
    "prom-client": "^11.5.3",
    "vue": "^2.6.10",
    "vue-highlight.js": "^3.1.0",
    "vue-router": "^3.0.6",
    "vue-scrollto": "^2.15.0",
    "vuex": "^3.1.1",
    "vuex-oidc": "^2.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "vue/no-parsing-error": [
        2,
        {
          "x-invalid-end-tag": false
        }
      ],
      "no-console": 0,
      "no-debugger": 0
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
